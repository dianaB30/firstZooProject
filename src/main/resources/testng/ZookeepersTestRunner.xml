<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ZookeepersTests" verbose="1" parallel="classes" thread-count="1">
    <groups>
        <run>
            <include name="functest"/>
        </run>
    </groups>
    <test name="tests.ZookeepersTests.testWorkingHoursMatch">
        <parameter name="zookeeperName" value="Nate"/>
        <parameter name="zookeeperNumber" value="0"/>
        <classes>
            <class name="tests.ZookeepersTests"/>
        </classes>
    </test>
</suite>
